{
  "manifest_version": 3,
  "version": "1.0",
  "name": "CTFhelper",
  "permissions": [
    "contextMenus",
    "clipboardRead",
    "clipboardWrite",
    "activeTab"
  ],
  "background": {
    "scripts": [ "main.js" ]

  },
  "host_permissions": [
    "http://*/*", // Allows access to all HTTP websites
    "https://*/*" // Allows access to all HTTPS websites
  ]


}
